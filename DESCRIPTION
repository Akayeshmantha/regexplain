    dplyr,
    rmarkdown,
    knitr,
    utils,
    tidyr,
    rstudioapi,
    shiny,
    miniUI,
    jsonlite,
    rematch2
RoxygenNote: 6.0.1
Roxygen: list(markdown = TRUE)
